{
  "dependencies": {
    "dotenv": "^16.3.1",
    "lodash": "^4.17.21",
    "openai": "^4.28.0",
    "tmi.js": "^1.8.5"
  },
  "devDependencies": {
    "@tsconfig/node16": "^1.0.2",
    "@types/dotenv": "^6.1.1",
    "@types/lodash": "^4.14.179",
    "@types/node": "^17.0.21",
    "@types/tmi.js": "^1.8.1",
    "ts-node": "^10.7.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.6.2"
  },
  "name": "twitchbot",
  "description": "a modernized bot for my twitch channel with OpenAI integration",
  "version": "2.0.0",
  "scripts": {
    "start": "node --experimental-specifier-resolution=node --loader ts-node/esm main.ts",
    "dev": "NODE_OPTIONS=--loader=ts-node/esm node main.ts",
    "dev:simple": "ts-node-dev --transpile-only main.ts",
    "dev:watch": "ts-node-dev --respawn --transpile-only main.ts",
    "build": "tsc",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "start:cjs": "npm run build:cjs && node dist-cjs/main.js",
    "fix-imports": "node fix-imports.js",
    "build:prod": "npm run build && npm run fix-imports",
    "start:prod": "node --experimental-specifier-resolution=node dist/main.js",
    "run": "node run-bot.cjs",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AKolumbic/twitchbot.git"
  },
  "author": "Andrew Kolumbic",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AKolumbic/twitchbot/issues"
  },
  "homepage": "https://github.com/AKolumbic/twitchbot#readme",
  "type": "module"
}
